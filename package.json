{
  "private": true,
  "scripts": {
    "start": "cross-env PORT=8848 umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "analyze": "cross-env ANALYZE=1 umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "umi": "^3.2.19"
  },
  "devDependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@umijs/fabric": "^2.2.2",
    "@umijs/plugin-access": "^2.3.1",
    "@umijs/plugin-antd": "^0.8.0",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/plugin-helmet": "^1.1.1",
    "@umijs/plugin-initial-state": "^2.2.1",
    "@umijs/plugin-locale": "^0.10.1",
    "@umijs/plugin-model": "^2.5.3",
    "@umijs/plugin-request": "^2.5.0",
    "@umijs/preset-ant-design-pro": "^1.2.2",
    "@umijs/yorkie": "^2.0.5",
    "cross-env": "^7.0.2",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1"
  },
  "engines": {
    "node": ">=13.5.0"
  }
}
