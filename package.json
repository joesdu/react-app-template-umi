{
  "private": true,
  "scripts": {
    "start": "cross-env PORT=8848 umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "analyze": "cross-env ANALYZE=1 umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^6.15.4",
    "antd": "^4.14.1",
    "classnames": "^2.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "umi": "3.4.4"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/express": "^4.17.11",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@umijs/fabric": "^2.5.7",
    "@umijs/plugin-access": "^2.3.3",
    "@umijs/plugin-antd": "^0.9.1",
    "@umijs/plugin-esbuild": "^1.1.0",
    "@umijs/plugin-helmet": "^1.1.1",
    "@umijs/plugin-initial-state": "^2.3.0",
    "@umijs/plugin-layout": "^0.14.1",
    "@umijs/plugin-locale": "^0.10.10",
    "@umijs/plugin-model": "^2.5.6",
    "@umijs/plugin-request": "^2.5.2",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/yorkie": "^2.0.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "engines": {
    "node": ">=15.11.0"
  }
}
