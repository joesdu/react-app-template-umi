{
  "private": true,
  "scripts": {
    "start": "cross-env PORT=8848 umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "analyze": "cross-env ANALYZE=1 umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-layout": "^6.5.15",
    "antd": "^4.8.5",
    "classnames": "^2.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "umi": "^3.2.28"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "^2.3.1",
    "@umijs/plugin-access": "^2.3.2",
    "@umijs/plugin-antd": "^0.9.0",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/plugin-helmet": "^1.1.1",
    "@umijs/plugin-initial-state": "^2.3.0",
    "@umijs/plugin-layout": "^0.12.5",
    "@umijs/plugin-locale": "^0.10.4",
    "@umijs/plugin-model": "^2.5.6",
    "@umijs/plugin-request": "^2.5.0",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/yorkie": "^2.0.5",
    "cross-env": "^7.0.2",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0"
  },
  "engines": {
    "node": ">=13.5.0"
  }
}
